{
    "name": "@ubio/framework",
    "version": "15.0.4",
    "type": "module",
    "description": "ubio Microservices Framework",
    "main": "out/main/index.js",
    "exports": {
        ".": "./out/main/index.js",
        "./modules/mongodb": "./out/modules/mongodb.js"
    },
    "files": [
        "out/main/**/*",
        "out/modules/**/*",
        "out/bin/**/*"
    ],
    "bin": {
        "generate-openapi": "out/bin/generate-openapi.js",
        "generate-docs": "out/bin/generate-docs.js",
        "generate-env": "out/bin/generate-env.js"
    },
    "scripts": {
        "dev": "npm run clean && tsc -w",
        "lint": "eslint --ext=.js,.ts --cache .",
        "test": "NODE_ENV=test mocha",
        "clean": "rm -rf out",
        "compile": "npm run clean && tsc",
        "preversion": "npm run compile && npm run lint && npm run test",
        "postversion": "npm publish --access public && git push origin v14 --tags"
    },
    "pre-commit": [
        "lint"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/ubio/node-framework.git"
    },
    "author": "ubio",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ubio/node-framework/issues"
    },
    "homepage": "https://github.com/ubio/node-framework#readme",
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/koa-compress": "^4.0.1",
        "@types/mocha": "^8.2.0",
        "@types/mongodb": "^3.6.4",
        "@types/node": "^14.14.32",
        "@types/supertest": "^2.0.10",
        "eslint": "^8.24.0",
        "mocha": "^10.0.0",
        "mongodb": "^4.4.1",
        "npm-run-all": "^4.1.5",
        "pre-commit": "^1.2.2",
        "supertest": "^6.1.3",
        "theredoc": "^1.0.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@koa/cors": "^3.1.0",
        "@nodescript/config": "^1.1.0",
        "@nodescript/eslint-config": "^1.0.4",
        "@nodescript/logger": "^1.4.1",
        "@nodescript/pathmatcher": "^1.0.2",
        "@types/koa": "^2.11.8",
        "@types/koa__cors": "^3.0.2",
        "@types/koa-conditional-get": "^2.0.0",
        "@types/koa-etag": "^3.0.0",
        "@types/node-fetch": "^2.5.8",
        "@types/stoppable": "^1.1.0",
        "@types/uuid": "^8.3.0",
        "@ubio/request": "^3.5.0",
        "ajv": "^8.1.0",
        "ajv-formats": "^2.0.2",
        "chalk": "^4.1.0",
        "commander": "^7.2.0",
        "dotenv": "^16.0.2",
        "jsonwebtoken": "^8.5.1",
        "koa": "^2.11.0",
        "koa-body": "^4.2.0",
        "koa-compress": "^5.0.1",
        "koa-conditional-get": "^3.0.0",
        "koa-etag": "^4.0.0",
        "mesh-ioc": "^3.1.4",
        "node-fetch": "^2.6.0",
        "reflect-metadata": "^0.1.13",
        "stoppable": "^1.1.0",
        "uuid": "^8.3.2"
    }
